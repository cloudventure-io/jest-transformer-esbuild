{
    "compilerOptions": {
        "module": "ES2020",
        "rootDir": "./",
        "outDir": "./dist",
        "baseUrl": "./src",
        "target": "ES2020",
        "esModuleInterop": true,
        "noImplicitAny": false,
        "sourceMap": false,
        "moduleResolution": "node",
        "types": ["node", "jest"],
        "declaration": true,
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true
    },
    "include": ["index.ts"]
}
